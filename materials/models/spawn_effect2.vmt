// NOTE: UnlitTwoTexture_DX9 no longer supports alpha blending (!?)
"UnlitTwoTexture_DX6"
{
 	"$model" 1
    "$nocull" "1"
	"$basetexture" "models/spawn_effect"
	"$texture2" "models/spawn_effect"

	"$additive" "1"

	"Proxies"
	{
		"TextureScroll"
		{
			"texturescrollvar" "$basetexturetransform"
			"texturescrollrate" 1.5
			"texturescrollangle" 90
		}
		"TextureScroll"
		{
			"texturescrollvar" "$texture2transform"
			"texturescrollrate" 3
			"texturescrollangle" 180
		}
	}
}
